<!DOCTYPE html>
<html>
	<head>
		<title></title>
		<link rel='stylesheet' href='/stylesheets/style.css' />
		<meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
	</head>
	<body>
	<% include ../partials/navadmin.ejs %>
	<div class="flexbox">
		<div class="userlist">
			<ol>

				<% anvandare.forEach(function(anv) { %>
				<li>
					<form action="anvandare" method="post">
						<input type="hidden" name="id" value="<%= anv.id %>">
						<button><%= anv.name %></button>
					</form>
				</li>
				<!--<li><a href="/admin/anvandare/<%= anv._id %>"><%= anv.name %></a></li>-->

				<% }); %>

			</ol>
		</div>
		<div class="userinfo">

			<div class="personalInfo">

				<div class="leftUserInfo">

					<p>Namn: <%= singel.name %></p>
					<p>Telefonenr: <%= singel.mobil %></p>
					<p>Avdelning: <%= singel.avdelning %></p>
					<p>Email: <%= singel.email %></p>
					<p>ID-nr: <%= singel.id %></p>
					<p>Förbrukade Km: <%= singel.timesPrivate %></p>

				</div>

				<div class="rightUserInfo">
				
					<p><a href="/admin/nyanvandare/"><button>Lägg till användare</button></a></p>
					<p><img src="/images/persona.jpg" alt="user" /></p>
					<p><a href="/admin/redigeraanvandare/<%= singel._id %>"><button>Administrera</button></a></p>

				</div>

			</div>


			<div class="bokningar">
				<p>Bokningar</p>
				<div class="bokdetatil"></div>
				<p>Kommande</p>
				<div class="bokdetatil"></div>
				<p>Tidigare</p>
				<div class="bokdetatil"></div>


			</div>
		</div>
	</div>

	</body>
</html>
